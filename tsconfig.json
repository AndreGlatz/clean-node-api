{
  "compilerOptions": {
    "target": "es5",                          
    "module": "commonjs",                     
    "sourceMap": true,                        
    "outDir": "./dist",                       
    "rootDir": "./src",
    "baseUrl": "./src",                       
    "paths": {
      "@/*": ["*"]
    },                           
    "esModuleInterop": true,                  
    "forceConsistentCasingInFileNames": true  
  },
  "include": [
    "src"
  ],
  "exclude": []
}
